{

  "manifest_version": 2,
  "name": "Activity Manager",
  "version": "1.0",

  "description": "Manages the websites you visit",
  "homepage_url": "https://github.com/codlocker/CodeMate/",
  "icons": {
    "48": "icons/large.jpg"
  },

  "permissions": [
    "history",
    "storage"
  ],

  "options_ui": {
    "page": "options.html"
  },

  "browser_action": {
    "default_icon": "icons/large.jpg",
    "default_title": "CodeMate",
    "default_popup": "popup/index.html"
  }

}